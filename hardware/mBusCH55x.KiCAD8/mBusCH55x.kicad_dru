(version 1)
(rule "class 0:0"
  (condition "A.NetClass == 'default' && B.NetClass == 'Default'")
  (constraint clearance (min 0.1524mm)))

(rule "class 1:1"
  (condition "A.NetClass == 'highpower' && B.NetClass == 'highpower'")
  (constraint clearance (min 0.2032mm)))

(rule "class 2:2"
  (condition "A.NetClass == 'USB' && B.NetClass == 'USB'")
  (constraint clearance (min 0.1524mm)))
